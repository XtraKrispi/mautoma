  "homepage": "https://xtrakrispi.github.io/mautoma",
  "scripts": {
    "start": "npm run server:dev & npm run tailwind:dev",
    "server:dev": "parcel index.html",
    "tailwind:dev": "tailwindcss -i ./src/input.css -o ./app.css --watch",
    "build": "parcel build ./index.html --dist-dir build --public-url https://xtrakrispi.github.io/mautoma",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -f"
  },
