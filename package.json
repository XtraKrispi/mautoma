    "@parcel/transformer-elm": "^2.13.3",
    "@tailwindcss/typography": "^0.5.16",
    "daisyui": "^5.0.9",
    "elm": "^0.19.1-6",
    "elm-format": "^0.8.7",
    "gh-pages": "^6.3.0",
    "parcel": "^2.13.3",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "parcel-transformer-elm-assets": "^1.0.3"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.16",
    "tailwindcss": "^4.0.16"
